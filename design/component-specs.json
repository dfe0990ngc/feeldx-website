{
  "designSystem": {
    "name": "FeelDX Design System",
    "version": "1.0.0",
    "colors": {
      "primary": "#86906C",
      "secondary": "#40472D",
      "dark": "#242424",
      "white": "#ffffff",
      "gray": {
        "50": "#f9fafb",
        "100": "#f3f4f6",
        "200": "#e5e7eb",
        "300": "#d1d5db",
        "600": "#4b5563",
        "700": "#374151",
        "800": "#1f2937",
        "900": "#111827"
      },
      "semantic": {
        "success": "#10b981",
        "warning": "#f59e0b",
        "error": "#ef4444",
        "info": "#3b82f6"
      }
    },
    "typography": {
      "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', sans-serif",
      "scale": {
        "h1": { "size": "3.5rem", "weight": 700, "lineHeight": 1.2 },
        "h2": { "size": "2.25rem", "weight": 700, "lineHeight": 1.2 },
        "h3": { "size": "1.5rem", "weight": 600, "lineHeight": 1.3 },
        "h4": { "size": "1.25rem", "weight": 600, "lineHeight": 1.3 },
        "body-large": { "size": "1.125rem", "weight": 400, "lineHeight": 1.5 },
        "body": { "size": "1rem", "weight": 400, "lineHeight": 1.5 },
        "body-small": { "size": "0.875rem", "weight": 400, "lineHeight": 1.5 },
        "caption": { "size": "0.75rem", "weight": 400, "lineHeight": 1.4 }
      }
    },
    "spacing": {
      "1": "0.25rem",
      "2": "0.5rem",
      "3": "0.75rem",
      "4": "1rem",
      "5": "1.25rem",
      "6": "1.5rem",
      "8": "2rem",
      "10": "2.5rem",
      "12": "3rem",
      "16": "4rem",
      "20": "5rem",
      "24": "6rem"
    },
    "borderRadius": {
      "sm": "0.375rem",
      "default": "0.5rem",
      "md": "0.75rem",
      "lg": "1rem",
      "xl": "1.5rem",
      "2xl": "2rem",
      "full": "9999px"
    },
    "shadows": {
      "sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
      "default": "0 1px 3px 0 rgb(0 0 0 / 0.1), 0 1px 2px -1px rgb(0 0 0 / 0.1)",
      "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
      "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
      "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)",
      "2xl": "0 25px 50px -12px rgb(0 0 0 / 0.25)"
    }
  },
  "components": {
    "buttons": {
      "primary": {
        "background": "#86906C",
        "color": "#ffffff",
        "padding": "1rem 2rem",
        "borderRadius": "0.5rem",
        "fontWeight": 600,
        "transition": "all 300ms",
        "hover": {
          "background": "#40472D",
          "transform": "scale(1.05)"
        }
      },
      "secondary": {
        "background": "transparent",
        "color": "#40472D",
        "border": "2px solid #40472D",
        "padding": "1rem 2rem",
        "borderRadius": "0.5rem",
        "fontWeight": 600,
        "transition": "all 300ms",
        "hover": {
          "background": "#40472D",
          "color": "#ffffff"
        }
      }
    },
    "cards": {
      "service": {
        "background": "#ffffff",
        "border": "1px solid #f3f4f6",
        "borderRadius": "0.75rem",
        "padding": "2rem",
        "transition": "all 300ms",
        "hover": {
          "borderColor": "#86906C",
          "boxShadow": "0 20px 25px -5px rgb(0 0 0 / 0.1)",
          "transform": "translateY(-8px)"
        }
      },
      "testimonial": {
        "background": "#ffffff",
        "borderRadius": "1.5rem",
        "padding": "2rem 3rem",
        "boxShadow": "0 25px 50px -12px rgb(0 0 0 / 0.25)",
        "position": "relative",
        "overflow": "hidden"
      }
    },
    "navigation": {
      "desktop": {
        "height": "4rem",
        "background": "rgba(255, 255, 255, 0.95)",
        "backdropFilter": "blur(8px)",
        "borderBottom": "1px solid #f3f4f6",
        "position": "fixed",
        "zIndex": 50
      },
      "mobile": {
        "background": "#ffffff",
        "borderTop": "1px solid #f3f4f6",
        "padding": "0.5rem"
      }
    }
  },
  "layout": {
    "container": {
      "maxWidth": "1280px",
      "margin": "0 auto",
      "padding": {
        "mobile": "0 1rem",
        "tablet": "0 1.5rem",
        "desktop": "0 2rem"
      }
    },
    "breakpoints": {
      "sm": "640px",
      "md": "768px",
      "lg": "1024px",
      "xl": "1280px",
      "2xl": "1536px"
    },
    "grids": {
      "services": {
        "columns": "repeat(auto-fit, minmax(300px, 1fr))",
        "gap": "2rem"
      },
      "testimonials": {
        "columns": "repeat(auto-fit, minmax(280px, 1fr))",
        "gap": "2rem"
      },
      "caseStudies": {
        "columns": "1fr 1fr",
        "gap": "3rem"
      }
    }
  },
  "animations": {
    "transitions": {
      "duration": "300ms",
      "timingFunction": "cubic-bezier(0.4, 0, 0.2, 1)"
    },
    "keyframes": {
      "fadeIn": {
        "from": { "opacity": 0 },
        "to": { "opacity": 1 }
      },
      "slideUp": {
        "from": { "transform": "translateY(20px)", "opacity": 0 },
        "to": { "transform": "translateY(0)", "opacity": 1 }
      },
      "pulse": {
        "0%, 100%": { "opacity": 1 },
        "50%": { "opacity": 0.5 }
      }
    }
  },
  "accessibility": {
    "colorContrast": {
      "text": "4.5:1",
      "largeText": "3:1",
      "interactive": "3:1"
    },
    "focusStates": {
      "outline": "none",
      "ring": "2px solid #86906C",
      "ringOffset": "2px"
    }
  },
  "darkMode": {
    "colors": {
      "background": "#111827",
      "surface": "#1f2937",
      "card": "#374151",
      "textPrimary": "#f9fafb",
      "textSecondary": "#d1d5db",
      "textMuted": "#9ca3af",
      "border": "#374151"
    },
    "transition": "300ms"
  }
}